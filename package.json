{
  "name": "@gosmsge/gosmsge-node",
  "version": "4.4.0",
  "description": "Sms send package for GOSMS.ge",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "/dist"
  ],
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev": "nodemon",
    "start": "npm run build && node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "prepublishOnly": "npm run build",
    "prepare": "husky",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "sms",
    "sms-api",
    "gosms.ge",
    "sms-sdk",
    "sms-gateway",
    "sms-client"
  ],
  "author": "gosms.ge",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gosms-ge/gosmsge-node.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gosms-ge/gosmsge-node/issues"
  },
  "homepage": "https://github.com/gosms-ge/gosmsge-node#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.9",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
